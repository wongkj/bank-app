{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./dist"
  },
  "include": [
    "./**/*.ts"
  ],
  "exclude": [
      "node_modules",
      "typings/**",
      "src/**/*.test.ts"
  ]  
}
